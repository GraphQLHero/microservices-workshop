{
  "name": "country-guesser",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsup src/index.ts --format cjs --out-dir dist",
    "//build:vercel": "See https://vercel.com/guides/using-express-with-vercel#standalone-express",
    "build:vercel": "yarn run build && mkdir -p api && cp dist/index.js api/index.js",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsup src/index.ts --format cjs --watch --onSuccess \"node dist/index.js\"",
    "lint": "tsc --noEmit && eslint \"src/**/*.ts*\"",
    "start": "node dist/index.js",
    "test": "jest --detectOpenHandles"
  },
  "jest": {
    "preset": "jest-presets/jest/node"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "logger": "workspace:*",
    "openai": "^3.2.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.22",
    "@types/node": "^15.12.2",
    "@types/supertest": "^2.0.12",
    "eslint": "^7.32.0",
    "eslint-config-custom-server": "workspace:*",
    "jest": "^26.6.3",
    "jest-presets": "workspace:*",
    "supertest": "^6.2.4",
    "tsconfig": "workspace:*",
    "tsup": "^6.2.3",
    "typescript": "^4.8.3"
  }
}
